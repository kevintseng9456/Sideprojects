<Dailycard>:
    orientation: 'vertical'
    size_hint_y: None
    height: "90dp"
    spacing: 2
    elevation: 0
    radius: [10]

    MDBoxLayout:
        id:card_box
        padding: 2
        # adaptive_height: True

        # FitImage:
        #     adaptive_height: True
        #     source: root.post

        FitImage:
            id:upload_or_ok_icon
            source:'./image/upload_icon.png'
            size_hint: None, None
            size: 53, 53
            radius: [53]
            # pos:[root.width,0]
        MDLabel:
            font_name:app.font_name  
            text: root.DailyMissionTitle

            
